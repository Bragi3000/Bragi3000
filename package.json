  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "linebreak-style": [
        "warn",
        "unix"
      ],
      "indent": [
        "warn",
        2
      ],
      "no-trailing-spaces": "warn",
      "eol-last": [
        "warn",
        "always"
      ],
      "import/no-duplicates": "error",
      "import/no-namespace": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
